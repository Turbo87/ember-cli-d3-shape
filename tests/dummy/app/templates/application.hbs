<h2 id="title">D3 Shape</h2>

{{my-component values=chartValues}}

<svg width="100" height="1000">
  <rect class="background" x="0" y="0" width="100" height="1000" />
  <g transform="translate(24,24)">
    {{#each symbols as |symbol index|}}
      {{primitive-symbol type=symbol size="100" top=(get symbolPositions symbol) fill="white"}}
    {{/each}}
  </g>
</svg>
